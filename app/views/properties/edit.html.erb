<%= form_with(model: @property, url: property_path, method: :patch ) do |form| %>
  <div>
      <%= form.label :title %>
      <br>
      <%= form.text_field :title %>
  </div>
    <div>
    <%= form.label :description%>
    <%= form.text_area :description%>
</div>
<div>
    <%= form.label :location%>
    <%= form.text_area :location%>
</div>
<div>
    <%= form.label :amenities%>
    <%= form.text_area :amenities%>
</div>
<div>
    <%= form.label :price%>
    <%= form.number_field :price%>
</div>

    <%= form.submit %>

    <% end %>